<%- layout('layouts/boilerplate.ejs') %>
    <form action="/campgrounds" method="post">
        <div>
            <label for="title">Title</label>
            <input type="text" name="campground[title]" id="title" placeholder="enter title">
        </div>
        <div>
            <label for="location">Location</label>
            <input type="text" name="campground[location]" id="location" placeholder="enter a location">
        </div>
        <button type="submit">Add Campground</button>
        <a href="/campgrounds">Back To Index</a>
    </form>
